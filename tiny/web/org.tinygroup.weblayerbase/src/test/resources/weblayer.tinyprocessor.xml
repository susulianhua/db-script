<tiny-processors>
    <tiny-processor id="myTinyProcessor" class="myTinyProcessor">
        <init-param name="name" value="xuanxuan"></init-param>
        <init-param name="age" value="1"></init-param>
        <servlet-mapping url-pattern=".*\.my"></servlet-mapping>
    </tiny-processor>
    <tiny-processor id="tinyProcessorWapper" class="tinyProcessorWapper">
        <init-param name="servlet_bean" value="flowServlet"></init-param>
        <servlet-mapping url-pattern=".*\.flow"></servlet-mapping>
    </tiny-processor>
</tiny-processors>