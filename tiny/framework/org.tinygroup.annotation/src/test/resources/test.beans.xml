<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName" default-lazy-init="true">
    <bean id="classAction" scope="singleton"
          class="org.tinygroup.annotation.action.AnnotationClassActionDemo">
    </bean>
    <bean id="methodAction" scope="singleton"
          class="org.tinygroup.annotation.action.AnnotationMethodActionDemo">
    </bean>
    <bean id="propertyAction" scope="singleton"
          class="org.tinygroup.annotation.action.AnnotationPropertyActionDemo">
    </bean>
</beans>
