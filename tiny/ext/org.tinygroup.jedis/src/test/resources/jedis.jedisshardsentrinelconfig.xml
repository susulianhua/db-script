<shard-jedis-sentinel-configs pool="jedisConfig">
    <shard-jedis-sentinel-config>
        <!--sentinels为redis主备的sentinel服务器，单个服务器地址为 ip:port,支持多个，多个之间逗号分隔 -->
        <!--sentinel服务器架设配置请参见redis相关说明 -->
        <shard-sentinel-configs sentinels="testdb:33333">
            <!-- sentinel的一个主备中所有的服务器的信息 master-name为该主备集群在sentinel中的标识 -->
            <!-- 注:此处要求所有*.jedisshardsentrinelconfig.xml中，master-name全局唯一 -->
            <shard-sentinel-config master-name="master1" password="www.tinygroup.org">
                <!-- id为集群内服务器标志，目前无实际含义,host为服务器ip，port为服务器地址 -->
                <jedis-config id="server01" host="testdb" port="11111" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server02" host="testdb" port="11112" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server03" host="testdb" port="11113" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server04" host="testdb" port="6379" password="www.tinygroup.org"></jedis-config>
            </shard-sentinel-config>
        </shard-sentinel-configs>

        <shard-sentinel-configs sentinels="testdb:43333">
            <shard-sentinel-config master-name="mymaster" password="www.tinygroup.org">
                <jedis-config id="server01" host="testdb" port="41111" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server02" host="testdb" port="41112" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server03" host="testdb" port="41113" password="www.tinygroup.org"></jedis-config>
                <jedis-config id="server04" host="testdb" port="46379" password="www.tinygroup.org"></jedis-config>
            </shard-sentinel-config>
        </shard-sentinel-configs>
    </shard-jedis-sentinel-config>
</shard-jedis-sentinel-configs>



