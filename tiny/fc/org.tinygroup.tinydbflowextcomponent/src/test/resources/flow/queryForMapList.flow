<flow id="queryForMapList" enable="true" private-context="false">
  <nodes>
    <node id="begin" name="begin" title="开始">
      <next-nodes>
        <next-node next-node-id="queryForMapListComponent"/>
      </next-nodes>
    </node>
    <node id="queryForMapListComponent" name="queryForMapListComponent" title="查询返回map list">
      <component name="queryForMapListComponent" title="查询返回map list">
        <description></description>
        <properties>
          <flow-property name="sql" value="${sql}" required="false" type="java.lang.String"/>
          <flow-property name="resultKey" value="result" required="false" type="java.lang.Object"/>
        </properties>
      </component>
      <next-nodes>
        <next-node next-node-id="end"/>
      </next-nodes>
    </node>
  </nodes>
</flow>