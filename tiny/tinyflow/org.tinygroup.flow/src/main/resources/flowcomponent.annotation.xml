<annotation-class-matchers>
    <annotation-class-matcher class-name="(.)*Component" annotation-id="flow"
                              annotation-type="org\.tinygroup\.flow\.annotation\.config\.ComponentDefine">
        <processor-beans>
            <processor-bean enable="true" name="flowComponentAnnotationAction">
            </processor-bean>
        </processor-beans>
        <annotation-property-matchers>
            <annotation-property-matcher
                    property-name=".*" annotation-type="org\.tinygroup\.flow\.annotation\.config\.ComponentParameter">
                <processor-beans>
                    <processor-bean enable="true" name="flowComponentAnnotationAction">
                    </processor-bean>
                </processor-beans>
            </annotation-property-matcher>
            <annotation-property-matcher
                    property-name=".*" annotation-type="org\.tinygroup\.flow\.annotation\.config\.ComponentResult">
                <processor-beans>
                    <processor-bean enable="true" name="flowComponentAnnotationAction">
                    </processor-bean>
                </processor-beans>
            </annotation-property-matcher>
        </annotation-property-matchers>
    </annotation-class-matcher>
</annotation-class-matchers>